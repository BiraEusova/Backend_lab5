@model Backend5.Models.Analysis

@{
    ViewData["Title"] = "Details";
}

<h2>Details of analysis for @ViewBag.Analysis.Patient.Name</h2>

<div>
    <h4>Analysis</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Type)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Type)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Date)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Date)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Status)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Status)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Lab)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Lab.Name)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-patientId="@ViewBag.Analysis.PatientId" asp-route-analysisId="@ViewBag.Analysis.AnalysisId">Edit</a> |
    <a asp-action="Index" asp-route-patientId="@ViewBag.Analysis.PatientId">Back to List</a>
</div>

